/* Memory Spaces Definitions, 448K flash, 64K ram */
/* Use bootloader starting at 0x0000 */
MEMORY
{
  rom (rx)  : ORIGIN = 0x000000001a000000, LENGTH = 0x0000000000080000
  prog (rx) : ORIGIN = 0x000000001b000000, LENGTH = 0x0000000000080000
  ram (rwx) : ORIGIN = 0x0000000010000000, LENGTH = 0x0000000000008000
}

MPU_MIN_ALIGN = 8K;
